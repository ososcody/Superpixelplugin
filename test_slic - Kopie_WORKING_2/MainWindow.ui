      <property name="html">
       <string>&lt;!DOCTYPE HTML PUBLIC &quot;-//W3C//DTD HTML 4.0//EN&quot; &quot;http://www.w3.org/TR/REC-html40/strict.dtd&quot;&gt;
&lt;html&gt;&lt;head&gt;&lt;meta name=&quot;qrichtext&quot; content=&quot;1&quot; /&gt;&lt;style type=&quot;text/css&quot;&gt;
p, li { white-space: pre-wrap; }
&lt;/style&gt;&lt;/head&gt;&lt;body style=&quot; font-family:'MS Shell Dlg 2'; font-size:7.8pt; font-weight:400; font-style:normal;&quot;&gt;
&lt;p style=&quot; margin-top:12px; margin-bottom:12px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px; background-color:#1d1d26;&quot;&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#676773;&quot;&gt;# https://gis.stackexchange.com/questions/128139/how-to-convert-float-raster-to-vector-with-python-gdal&lt;br /&gt;#  Import the ogr and gdal modules and go straight ahead&lt;br /&gt;#  and open the raster we want to convert by passing it the filename on disk and getting a raster band:&lt;br /&gt;&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#cc7832;&quot;&gt;from &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;osgeo &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#cc7832;&quot;&gt;import &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;ogr&lt;br /&gt;&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#cc7832;&quot;&gt;from &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;osgeo &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#cc7832;&quot;&gt;import &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;gdal&lt;br /&gt;&lt;br /&gt;raster_path = &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#4dacf0;&quot;&gt;0  &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#676773;&quot;&gt;#&lt;br /&gt;&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;shapefile_path = &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#4dacf0;&quot;&gt;0  &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#676773;&quot;&gt;#&lt;br /&gt;&lt;br /&gt;&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;source_raster = gdal.Open(raster_path)&lt;br /&gt;band = gdal.Open(raster_path).GetRasterBand(&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#4dacf0;&quot;&gt;1&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;)&lt;br /&gt;driver = ogr.GetDriverByName(&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#62a362;&quot;&gt;'ESRI Shapefile'&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;)&lt;br /&gt;out_data = driver.CreateDataSource(shapefile_path)&lt;br /&gt;&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#676773;&quot;&gt;# getting projection from source raster&lt;br /&gt;&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;srs = ogr.SpatialReference()&lt;br /&gt;srs.ImportFromWkt(source_raster.GetProjectionRef())&lt;br /&gt;&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#676773;&quot;&gt;# create layer with projection&lt;br /&gt;&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;out_layer = out_data.CreateLayer(raster_path.split(&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#62a362;&quot;&gt;'.'&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;)[&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#4dacf0;&quot;&gt;0&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;]&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; font-weight:600; color:#e0957b;&quot;&gt;, &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;srs)&lt;br /&gt;new_field = ogr.FieldDefn(&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#62a362;&quot;&gt;'field_name'&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; font-weight:600; color:#e0957b;&quot;&gt;, &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;ogr.OFTReal)&lt;br /&gt;out_layer.CreateField(new_field)&lt;br /&gt;gdal.FPolygonize(band&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; font-weight:600; color:#e0957b;&quot;&gt;, &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;band&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; font-weight:600; color:#e0957b;&quot;&gt;, &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;out_layer&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; font-weight:600; color:#e0957b;&quot;&gt;, &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#4dacf0;&quot;&gt;0&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; font-weight:600; color:#e0957b;&quot;&gt;, &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;[]&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; font-weight:600; color:#e0957b;&quot;&gt;, &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#aa4926;&quot;&gt;callback&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;=&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#cc7832;&quot;&gt;None&lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#c9c9d1;&quot;&gt;)&lt;br /&gt;out_data.Destroy()&lt;br /&gt;source_raster = &lt;/span&gt;&lt;span style=&quot; font-family:'JetBrains Mono,monospace'; color:#cc7832;&quot;&gt;None&lt;/span&gt;&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;</string>
